<%- include('partials/header'); -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <div class="mb-3">
                <label  class="form-label">Title</label>
                <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="titleText">
            </div>
            <div class="mb-3">
                <label  class="form-label">Post</label>
                <textarea class="form-control" aria-label="With textarea" cols="40" rows="5" name="postText"></textarea>
            </div>
        </div>
        <button type="submit" name=" button" class="btn btn-primary">Publish</button>
    </form>
<%- include('partials/footer'); -%>
